<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/level02bg"
    tools:context=".Level_02_2">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="19dp"
        android:layout_marginBottom="68dp"
        android:background="#ec2453"
        app:layout_constraintBottom_toTopOf="@+id/txtTimer"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="19dp"
        android:layout_marginBottom="68dp"
        android:gravity="center"
        android:text="devide by 4"
        android:textColor="#fafafa"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txtTimer"
        tools:layout_editor_absoluteX="16dp" />


    <FrameLayout
        android:id="@+id/boxLayout"
        android:layout_width="145dp"
        android:layout_height="111dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/parentLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.055">


        <ImageView
            android:id="@+id/imgBox"
            android:layout_width="159dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:src="@drawable/box"
            app:layout_constraintBottom_toBottomOf="@+id/boxLayout"
            app:layout_constraintStart_toStartOf="@+id/boxLayout"
            tools:layout_editor_absoluteX="107dp"
            tools:layout_editor_absoluteY="78dp" />


    </FrameLayout>


    <!--lower images start-->


    <ImageView
        android:id="@+id/imgNum_16"
        android:layout_width="91dp"
        android:layout_height="98dp"
        android:src="@drawable/n16"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imgNum_24"
        android:layout_width="71dp"
        android:layout_height="85dp"
        android:layout_marginStart="52dp"
        android:layout_marginBottom="164dp"
        android:src="@drawable/n24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgNum_16" />

    <ImageView
        android:id="@+id/imgNum_36"
        android:layout_width="77dp"
        android:layout_height="98dp"
        android:layout_marginStart="164dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/n36"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imgNum_16" />

    <ImageView
        android:id="@+id/imgNum_9"
        android:layout_width="78dp"
        android:layout_height="84dp"
        android:layout_marginStart="76dp"
        android:layout_marginBottom="128dp"
        android:src="@drawable/n9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imgNum_36" />

    <ImageView
        android:id="@+id/imgNum_15"
        android:layout_width="82dp"
        android:layout_height="80dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="172dp"
        android:src="@drawable/n15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imgNum_16" />

    <TextView
        android:id="@+id/txtScore"
        android:layout_width="28dp"
        android:layout_height="18dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtScr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.91"
        app:layout_constraintStart_toEndOf="@+id/boxLayout" />

    <TextView
        android:id="@+id/txtTimer"
        android:layout_width="74dp"
        android:layout_height="33dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/boxLayout"
        app:layout_constraintHorizontal_bias="0.718"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtScr"
        android:layout_width="37dp"
        android:layout_height="17dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:text="Score"
        app:layout_constraintStart_toEndOf="@+id/boxLayout"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnOk"
        android:layout_width="55dp"
        android:layout_height="34dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/ok_button"
        android:text="OK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <!--lower images end-->


    <FrameLayout
        android:id="@+id/parentLayout"
        android:layout_width="414dp"
        android:layout_height="323dp"
        android:layout_marginTop="122dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">


    </FrameLayout>
</android.support.constraint.ConstraintLayout>